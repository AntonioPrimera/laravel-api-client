{
    "name": "antonioprimera/laravel-api-client",
    "description": "Laravel Api Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio",
            "email": "antonio@cus.ro"
        }
    ],
    "require-dev": {
        "antonioprimera/lara-packager": "^1.4",
        "orchestra/testbench": "^6.0",
        "guzzlehttp/guzzle": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "AntonioPrimera\\ApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntonioPrimera\\ApiClient\\Tests\\": "tests/"
        }
    },
    "require": {
        "spatie/laravel-package-tools": "^1.9"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AntonioPrimera\\ApiClient\\LaravelApiClientServiceProvider"
            ]
        }
    }
}
